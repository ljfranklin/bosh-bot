---
pidfile: /var/vcap/sys/run/bosh-bot/bosh-bot.pid
slack:
  token: <%= p('slack.token') %>
  authorized_usernames: <%= p('slack.authorized_usernames') %>
  authorized_channels: <%= p('slack.authorized_channels') %>
  notification_channel_id: <%= p('slack.notification_channel_id', nil) %>
bosh:
  env: <%= p('bosh.env') %>
  user: <%= p('bosh.user') %>
  password: <%= p('bosh.password') %>
  releases: <%= p('bosh.releases').to_json %>
  stemcells: <%= p('bosh.stemcells').to_json %>
  deployments: <%= p('bosh.deployments').to_json %>
  assets: <%= p('bosh.assets').to_json %>
